	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="cancelQueryController" class="com.ips.bmps.module.setmain.cancel.web.CancelQueryController"
		parent="baseController">
		<property name="drawService" ref="drawService" />
 		<property name="cancelService" ref="cancelService" />
 		<property name="refundService" ref="refundService" />
