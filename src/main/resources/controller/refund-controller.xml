<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="refundController" class="com.ips.bmps.module.setmain.refund.web.RefundController"
		parent="baseController">
		<property name="refundService" ref="refundService" />
		<property name="drawFileService" ref="drawFileService" />
		<property name="refundTemplate" ref="refundTemplate" />
	</bean>

	<bean id="refundQueryController"
		class="com.ips.bmps.module.setmain.refund.web.RefundQueryController"
		parent="baseController">
		<property name="refundService" ref="refundService" />
		<property name="drawFileService" ref="drawFileService" />
	</bean>


</beans>